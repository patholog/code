<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.pathology.entity.Annotation" table="annotation" catalog="pathology">
    <composite-id>
      <key-property name="imageId" type="java.lang.Integer">
        <column name="image_id"/>
      </key-property>
      <key-property name="name" type="java.lang.String">
        <column name="name"/>
      </key-property>
    </composite-id>
    <property name="positionX" type="java.lang.Integer">
      <column name="position_x"/>
    </property>
    <property name="positionY" type="java.lang.Integer">
      <column name="position_y"/>
    </property>
    <property name="crtTime" type="java.util.Date">
      <column name="crt_time" length="0"/>
    </property>
  </class>
</hibernate-mapping>
